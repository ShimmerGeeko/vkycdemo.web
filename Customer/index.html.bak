    <script>
        window.onload = function () {
            sessionStorage.clear()
        }

        function next() {
            document.getElementById('rowWelcome').style.display = 'none';
            // document.getElementById('divNext').style.display = 'none';
            document.getElementById('rowRequire').style.display = 'block';
            document.getElementById('divPrev').style.display = 'block';
            document.getElementById('divProceed').style.display = 'block';
        }
        function prev() {
            document.getElementById('rowRequire').style.display = 'none';
            document.getElementById('divPrev').style.display = 'none';
            document.getElementById('divProceed').style.display = 'none';
            // document.getElementById('divNext').style.display = 'block';
            document.getElementById('rowWelcome').style.display = 'block';

        }
        function proceed() {
            var ApplType = sessionStorage.getItem('sesParamApplType');
            if (ApplType === '1') {
                window.location.href = "./CustIndex.html";
            }
            else if (ApplType === '2') {
                window.location.href = "./CustIndexAcc.html";
            }
            else if (ApplType === '3') {
                window.location.href = "./CustIndex.html";
            }
            else if (ApplType === '5') {
            }
            else if (ApplType === '4') {
                window.location.href = "./CustIndexReKYC.html";
            }
        }

        var btnBasicKYC = document.getElementById('btnBasicKYC');
        btnBasicKYC.addEventListener('click', () => {
            sessionStorage.setItem('sesParamApplType', '1');
            document.getElementById('icoAccOpening').style.display = 'none';
            document.getElementById('icoApplyLoan').style.display = 'none';
            document.getElementById('icoReKyc').style.display = 'none';
            document.getElementById('icoCC').style.display = 'none';
            document.getElementById('icoBKyc').style.display = 'inline-block';
            routeToNext();
        })

        var btnAccOpening = document.getElementById('btnAccOpening');
        btnAccOpening.addEventListener('click', () => {
            sessionStorage.setItem('sesParamApplType', '2');
            document.getElementById('icoAccOpening').style.display = 'inline-block';
            document.getElementById('icoApplyLoan').style.display = 'none';
            document.getElementById('icoReKyc').style.display = 'none';
            document.getElementById('icoCC').style.display = 'none';
            document.getElementById('icoBKyc').style.display = 'none';
            routeToNext();
        })

        /*var btnGetLoan = document.getElementById('btnGetLoan');
        btnGetLoan.addEventListener('click', () => {
            swal('Coming Soon...', '', 'info');
            sessionStorage.setItem('sesParamApplType', '5');
            document.getElementById('icoAccOpening').style.display = 'none';
            document.getElementById('icoApplyLoan').style.display = 'inline-block';
            document.getElementById('icoReKyc').style.display = 'none';
            routeToNext();
        })*/

        var btnCC = document.getElementById('btnCC');
        btnCC.addEventListener('click', () => {
            sessionStorage.setItem('sesParamApplType', '3');
            document.getElementById('icoAccOpening').style.display = 'none';
            document.getElementById('icoApplyLoan').style.display = 'none';
            document.getElementById('icoReKyc').style.display = 'none';
            document.getElementById('icoCC').style.display = 'inline-block';
            document.getElementById('icoBKyc').style.display = 'none';
            routeToNext();
        })

        var btnReKyc = document.getElementById('btnReKyc');
        btnReKyc.addEventListener('click', () => {
            sessionStorage.setItem('sesParamApplType', '4');
            document.getElementById('icoAccOpening').style.display = 'none';
            document.getElementById('icoApplyLoan').style.display = 'none';
            document.getElementById('icoReKyc').style.display = 'inline-block';
            document.getElementById('icoCC').style.display = 'none';
            document.getElementById('icoBKyc').style.display = 'none';
            routeToNext();
        })

        function routeToNext() {
            next();
        }
    </script>
